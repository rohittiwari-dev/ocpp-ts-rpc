{
	"name": "ocpp-rpc-ts",
	"version": "1.0.0",
	"description": "A Node.js client & server implementation of the WAMP-like RPC-over-websocket system defined in the OCPP-J protocols. Inspired By @mikuso/ocpp-rpc",
	"main": "dist/serverExample.js",
	"scripts": {
		"dev": "ts-node-dev --respawn --transpile-only src/serverExample.ts",
		"build": "tsc && copyfiles -f ./src/lib/schemas/* ./dist/lib/schemas/",
		"start": "node dist/serverExample.js"
	},
	"engines": {
		"node": ">=17.3.0"
	},
	"keywords": [
		"ocpp",
		"rpc",
		"websocket rpc",
		"wamp",
		"websockets",
		"ocpp-j",
		"ocpp1.6",
		"ocpp2.0.1",
		"ocpp1.6j",
		"ocpp2.0.1j",
		"rpc server",
		"rpc client",
		"ocpp rpc",
		"ocpp websockets",
		"ocpp rpc typescript",
		"rpc typescript",
		"ocpp typescript",
		"ocpp node js typescript"
	],
	"author": {
		"name": "Rohit Tiwari",
		"url": "https://github.com/rohittiwari-dev"
	},
	"bugs": {
		"url": "https://github.com/rohittiwari-dev/ocpp-ts-rpc/issues"
	},
	"repository": "github:rohittiwari-dev/ocpp-ts-rpc",
	"homepage": "https://github.com/rohittiwari-dev/ocpp-ts-rpc#readme",
	"license": "MIT",
	"devDependencies": {
		"@types/backoff": "^2.5.5",
		"@types/node": "^20.11.19",
		"copyfiles": "^2.4.1",
		"nyc": "^15.1.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"ajv": "^8.12.0",
		"ajv-formats": "^2.1.1",
		"backoff": "^2.5.0",
		"ws": "^8.16.0"
	}
}
